{
	"name": "chat",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/chat/src",
	"projectType": "application",
	"tags": [],
	"targets": {
		"build": {
			"executor": "@nx/vite:build",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "production",
			"options": {
				"outputPath": "dist/apps/chat",
				"configFile": "apps/chat/vite.config.ts"
			},
			"configurations": {
				"development": {
					"mode": "development"
				},
				"production": {
					"mode": "production",
					"fileReplacements": [
						{
							"replace": "apps/chat/src/environments/environment.ts",
							"with": "apps/chat/src/environments/environment.prod.ts"
						}
					]
				},
				"production-fresh": {
					"mode": "production",
					"fileReplacements": [
						{
							"replace": "apps/chat/src/environments/environment.ts",
							"with": "apps/chat/src/environments/environment.prod.ts"
						}
					],
					"skipNxCache": true
				}
			}
		},
		"serve": {
			"executor": "@nx/vite:dev-server",
			"defaultConfiguration": "development",
			"options": {
				"buildTarget": "chat:build",
				"hmr": true
			},
			"configurations": {
				"development": {
					"buildTarget": "chat:build:development"
				},
				"production": {
					"buildTarget": "chat:build:production",
					"hmr": false
				}
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"]
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "apps/chat/jest.config.ts"
			}
		}
	}
}
