{
  "name": "discover",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/discover/src",
  "projectType": "application",
  "targets": {
		"build": {
			"executor": "@nx/vite:build",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "production",
			"options": {
				"outputPath": "dist/apps/discover",
				"configFile": "apps/discover/vite.config.ts"
			},
			"configurations": {
				"development": {
					"mode": "development"
				},
				"production": {
					"mode": "production"
				}
			}
		},
		"serve": {
			"executor": "@nx/vite:dev-server",
			"defaultConfiguration": "development",
			"options": {
				"buildTarget": "discover:build",
				"port": 4200,
				"host": "127.0.0.1"
			},
			"configurations": {
				"development": {
					"buildTarget": "discover:build:development",
					"hmr": true
				},
				"production": {
					"buildTarget": "discover:build:production",
					"hmr": false
				}
			}
		},
		"preview": {
			"executor": "@nx/vite:preview-server",
			"defaultConfiguration": "development",
			"options": {
				"buildTarget": "discover:build",
				"port": 4300,
				"host": "localhost"
			},
			"configurations": {
				"development": {
					"buildTarget": "discover:build:development"
				},
				"production": {
					"buildTarget": "discover:build:production"
				}
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"]
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "apps/discover/jest.config.ts"
			}
		}
  },
  "tags": []
}
