		"build:desktop:app": "nx run desktop:build",
		"build:desktop:renderer": "nx build chat --prod --baseHref=./",
		"build:desktop": "yarn build:desktop:app && yarn build:desktop:renderer",
		"package:desktop": "nx run desktop:package",
		"make:desktop": "nx run desktop:make",
		"release:desktop": "yarn build:desktop && yarn package:desktop && yarn make:desktop"
